{
  "name": "Forum embeds",
  "description": "Allows you to embed projects, audio and video in your forum posts. This works by adding a [url=ex-embed][/url] tag before your embed links.",
  "userscripts": [
    {
      "url": "embeds.js",
      "matches": [
        "https://scratch.mit.edu/discuss/topic/*",
        "https://scratch.mit.edu/discuss/post/*/edit",
        "https://scratch.mit.edu/discuss/settings/*"
      ],
      "runAtComplete": false
    },
    {
      "url": "embedbutton.js",
      "matches": [
        "https://scratch.mit.edu/discuss/topic/*",
        "https://scratch.mit.edu/discuss/post/*/edit",
        "https://scratch.mit.edu/discuss/settings/*"
      ],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": [
        "https://scratch.mit.edu/discuss/topic/*",
        "https://scratch.mit.edu/discuss/post/*/edit",
        "https://scratch.mit.edu/discuss/settings/*"
      ]
    }
  ],
  "credits": [
    {
      "name": "CST1229",
      "link": "https://github.com/CST1229"
    }
  ],
  "info": [
    {
      "type": "notice",
      "text": "Users without this addon enabled will see normal links instead of embeds.",
      "id": "normal-links"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "tags": ["community", "forums"]
}
